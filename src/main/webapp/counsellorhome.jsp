    <a href="#home" class="w3-bar-item w3-button w3-wide">Counseling and Visitor management</a>
    <!-- Right-sided navbar links -->
    <div class="w3-right w3-hide-small">
      <a href="/" class="w3-bar-item w3-button">HOME</a>
      <a href="viewmen" class="w3-bar-item w3-button"><i class="fa fa-user"></i> View Profile</a>
      <a href="echangepwd"" class="w3-bar-item w3-button"><i class="fa fa-th"></i> Change Password</a>
      <a href="counsellorlogin" class="w3-bar-item w3-button"><i class="fa fa-usd"></i>Logout</a>
      </div>
    <!-- Hide right-floated links on small screens and replace them with a menu icon -->

    <a href="javascript:void(0)" class="w3-bar-item w3-button w3-right w3-hide-large w3-hide-medium" onclick="w3_open()">
      <i class="fa fa-bars"></i>
    </a>
  </div>
</div>


